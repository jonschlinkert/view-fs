{
  "name": "view-fs",
  "description": "View plugin for `templates` that adds methods for working with the file system. Works with assemble, verb, and any other `templates` based applications",
  "version": "0.2.0",
  "homepage": "https://github.com/jonschlinkert/view-fs",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/view-fs",
  "bugs": {
    "url": "https://github.com/jonschlinkert/view-fs/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "delete": "^0.3.2",
    "extend-shallow": "^2.0.1",
    "lazy-cache": "^2.0.1",
    "read-file": "^0.2.0",
    "write": "^0.3.1"
  },
  "devDependencies": {
    "fs-exists-sync": "^0.1.0",
    "gulp-format-md": "^0.1.7",
    "mocha": "^2.4.5",
    "templates": "^0.15.5"
  },
  "keywords": [
    "assemble",
    "copy",
    "del",
    "delete",
    "file",
    "fs",
    "move",
    "path",
    "plugin",
    "read",
    "templates",
    "verb",
    "vinyl",
    "write"
  ],
  "verb": {
    "run": true,
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "delete",
        "write",
        "read-file"
      ]
    },
    "reflinks": [
      "verb"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
